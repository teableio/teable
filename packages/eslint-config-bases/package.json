{
  "name": "@teable-group/eslint-config-bases",
  "version": "1.5.3",
  "license": "AGPL-3.0",
  "private": true,
  "homepage": "https://github.com/teable-group/teable",
  "repository": {
    "type": "git",
    "url": "https://github.com/teable-group/teable",
    "directory": "packages/eslint-config-bases"
  },
  "author": {
    "name": "tea artist",
    "url": "https://github.com/tea-artist"
  },
  "type": "commonjs",
  "main": "./src/index.js",
  "exports": {
    ".": {
      "require": "./src/index.js"
    },
    "./patch/modern-module-resolution": {
      "require": "./src/patch/modern-module-resolution.js"
    },
    "./helpers": {
      "require": "./src/helpers/index.js"
    },
    "./jest": {
      "require": "./src/bases/jest.js"
    },
    "./playwright": {
      "require": "./src/bases/playwright.js"
    },
    "./prettier": {
      "require": "./src/bases/prettier.js"
    },
    "./react": {
      "require": "./src/bases/react.js"
    },
    "./rtl": {
      "require": "./src/bases/rtl.js"
    },
    "./regexp": {
      "require": "./src/bases/regexp.js"
    },
    "./sonar": {
      "require": "./src/bases/sonar.js"
    },
    "./storybook": {
      "require": "./src/bases/storybook.js"
    },
    "./tailwind": {
      "require": "./src/bases/tailwind.js"
    },
    "./typescript": {
      "require": "./src/bases/typescript.js"
    }
  },
  "scripts": {
    "clean": "rimraf --no-glob ./dist ./coverage ./tsconfig.tsbuildinfo",
    "lint": "eslint  --ext .ts,.js,.cjs,.mjs --cache --cache-location ../../.cache/eslint/eslint-config-bases.eslintcache",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "fix-all-files": "eslint  --ext .ts,.tsx,.js,.jsx --fix"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-playwright": "^0.11.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.9.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "eslint-plugin-storybook": "^0.6.6",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-testing-library": "^5.9.1",
    "prettier": "^2.7.1"
  },
  "peerDependencies": {
    "eslint": "^8.22.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.7.4"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jest": "29.2.0",
    "@types/node": "18.11.7",
    "@types/prettier": "2.7.1",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "eslint": "8.26.0",
